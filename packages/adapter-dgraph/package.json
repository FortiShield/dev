{
  "name": "@auth/dgraph-adapter",
  "version": "2.7.4",
  "description": "Dgraph adapter for Auth.js",
  "homepage": "https://auth.khulnasoft.com",
  "repository": "https://github.com/khulnasoft/nextdev",
  "bugs": {
    "url": "https://github.com/khulnasoft/nextdev/issues"
  },
  "author": "Arnaud Derbey <arnaud@derbey.dev>",
  "contributors": [
    "KhulnaSoft, Ltd <info@khulnasoft.com>"
