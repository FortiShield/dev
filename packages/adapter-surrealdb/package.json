  },
  "scripts": {
    "test": "./test/test.sh",
    "build": "tsc",
    "clean": "rm -rf *.js *.d.ts*"
  },
  "dependencies": {
    "@auth/core": "workspace:*"
  },
  "peerDependencies": {
    "surrealdb.js": "^0.11.0"
  }
}
