{
  "name": "@auth/surrealdb-adapter",
  "version": "1.7.4",
  "description": "SurrealDB adapter for next-auth.",
  "homepage": "https://auth.khulnasoft.com",
  "repository": "https://github.com/khulnasoft/nextdev",
  "bugs": {
    "url": "https://github.com/khulnasoft/nextdev/issues"
  },
  "author": "Martin Schaer <martin@schaerweb.com>",
  "contributors": [
    "Md Sulaiman <dev.sulaiman@icloud.com>"
  ],
  "type": "module",
  "types": "./index.d.ts",
  "files": [
