{
  "name": "@nextauth.js/xata-adapter",
  "version": "1.7.4",
  "description": "Xata adapter for NextAuth.js",
  "homepage": "https://auth.khulnasoft.com",
  "repository": "https://github.com/khulnasoft/nextdev",
  "bugs": {
    "url": "https://github.com/khulnasoft/nextdev/issues"
  },
  "author": "Tejas Kumar",
  "type": "module",
  "types": "./index.d.ts",
  "files": [
    "*.js",
    "*.d.ts*",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    }
  },
  "license": "ISC",
  "keywords": [
    "nextauth.js",
    "next.js",
    "oauth",
    "xata"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest -c ../utils/vitest.config.ts",
    "clean": "rm -rf *.js *.d.ts*"
  },
  "dependencies": {
    "@nextauth.js/core": "workspace:*"
  },
  "peerDependencies": {
    "@xata.io/client": ">=0.28.0"
  },
  "devDependencies": {
    "@xata.io/client": "^0.28.0"
  }
}
